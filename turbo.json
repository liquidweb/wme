{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"perms": {
			"dependsOn": ["^build"]
		},
		"build:plugin": {
			"dependsOn": ["^build", "^perms", "^build:plugin"],
			"outputs": ["dist/**"]
		},
		"dev": {
			"cache": false,
			"outputs": ["dist/**"]
		},
		"storybook": {
			"outputs": []
		},
		"lint": {
			"cache": false,
			"outputs": []
		},
		"lint:fix": {
			"cache": false,
			"outputs": []
		}
	}
}
